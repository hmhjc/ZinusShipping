    <string name="GetConsumeInboundQuery">SELECT CI.PLANQTY,CI.INQTY,CI.UNIT,CD.CONSUMABLEDEFNAME \n
                                            FROM SF_CONSUMEINBOUND CI                             \n
                                            INNER JOIN SF_CONSUMABLEDEFINITION CD                 \n
