    <!--StockCheck-->
    <string name="GetStockCheckQuery">SELECT * FROM SF_STOCKCHECK \n WHERE 1=1 \n AND CHECKMONTH  = \'%1$s\' \n</string>
    <string name="gscConditionWarehouse">AND WAREHOUSEID = \'%1$s\' \n</string>
    <string name="gscConditionState">AND STATE = \'%1$s\' \n</string>
    <string name="gscOrderBy">ORDER BY CHECKMONTH,WAREHOUSEID,STATE</string>

    <string name="GetStockCheckDetailQuery">SELECT SC.WAREHOUSEID, SC.UNIT, SC.QTY, SC.CHECKQTY, CD.CONSUMABLEDEFNAME \n
                                            FROM SF_STOCKCHECKDETAIL SC \n
                                            INNER JOIN SF_CONSUMABLEDEFINITION CD ON CD.CONSUMABLEDEFID = SC.CONSUMABLEDEFID \n
                                                                                 AND CD.CONSUMABLEDEFVERSION = SC.CONSUMABLEDEFVERSION \n
                                            WHERE 1=1 AND CHECKMONTH = \'%1$s\' AND WAREHOUSEID = \'%2$s\'
    </string>

    <string name="GetStockLotCheckDetailQuery">SELECT SLC.WAREHOUSEID, SLC.CONSUMABLELOTID, SLC.UNIT, SLC.QTY, SLC.CHECKQTY, CD.CONSUMABLEDEFNAME \n
                                               FROM SF_STOCKLOTCHECKDETAIL SLC \n
                                               INNER JOIN SF_CONSUMABLEDEFINITION CD ON CD.CONSUMABLEDEFID = SLC.CONSUMABLEDEFID \n
                                                                                    AND CD.CONSUMABLEDEFVERSION = SLC.CONSUMABLEDEFVERSION \n
                                            WHERE 1=1 AND CHECKMONTH = \'%1$s\' AND WAREHOUSEID = \'%2$s\'
    </string>

    <!--Inbound-->
    <string name="GetInboundOrderQuery">SELECT * FROM SF_INBOUNDORDER \n WHERE 1=1 \n AND SCHEDULEDATE  &gt; \'%1$s\' AND SCHEDULEDATE &lt;  \'%2$s\' \n</string>
    <string name="gioConditionConsumabledefId">AND WAREHOUSEID = \'%1$s\' \n</string>
    <string name="gioConditionState">AND INBOUNDSTATE = \'%1$s\' \n</string>
    <string name="gioOrderBy">ORDER BY INBOUNDNO,WAREHOUSEID,INBOUNDSTATE</string>

    <string name="GetConsumeLotInboundQuery">SELECT CLI.PLANQTY,CLI.INQTY,CLI.UNIT,CLI.CONSUMABLELOTID,CD.CONSUMABLEDEFNAME \n
                                            FROM SF_CONSUMELOTINBOUND CLI \n
                                            INNER JOIN SF_CONSUMABLEDEFINITION CD ON CD.CONSUMABLEDEFID = CLI.CONSUMABLEDEFID \n
                                                                                 AND CD.CONSUMABLEDEFVERSION = CLI.CONSUMABLEDEFVERSION  \n
                                            WHERE 1=1 AND INBOUNDNO = \'%1$s\'
                                            ORDER BY CD.CONSUMABLEDEFNAME,CLI.CONSUMABLELOTID
    </string>

    <string name="GetConsumeInboundQuery">SELECT CI.PLANQTY,CI.INQTY,CI.UNIT,CD.CONSUMABLEDEFNAME \n
                                            FROM SF_CONSUMEINBOUND CI                             \n
                                            INNER JOIN SF_CONSUMABLEDEFINITION CD                 \n
                                            ON CD.CONSUMABLEDEFID = CI.CONSUMABLEDEFID            \n
                                            AND CD.CONSUMABLEDEFVERSION = CI.CONSUMABLEDEFVERSION \n
                                            WHERE 1=1 AND INBOUNDNO = \'%1$s\'                    \n
                                            ORDER BY CD.CONSUMABLEDEFNAME
    </string>
    <!--ConsumeRequest-->
    <string name="GetConsumeRequestQuery">SELECT * FROM SF_CONSUMEREQUEST \n WHERE 1=1 \n AND REQUESTDATE  &gt; \'%1$s\' AND REQUESTDATE &lt;  \'%2$s\' \n</string>
    <string name="gcrConditionAreaId">AND AREAID = \'%1$s\' \n</string>
    <string name="gcrConditionState">AND OUTBOUNDSTATE = \'%1$s\' \n</string>
    <string name="gcrOrderBy">ORDER BY CONSUMEREQNO,AREAID,OUTBOUNDSTATE</string>

    <string name="GetConsumeLotOutboundQuery">SELECT CLO.REQUESTQTY,CLO.OUTQTY,CLO.UNIT,CLO.CONSUMABLELOTID,CD.CONSUMABLEDEFNAME \n
                                            FROM SF_CONSUMELOTOUTBOUND CLO \n
                                            INNER JOIN SF_CONSUMABLEDEFINITION CD ON CD.CONSUMABLEDEFID = CLO.CONSUMABLEDEFID \n
                                                                                 AND CD.CONSUMABLEDEFVERSION = CLO.CONSUMABLEDEFVERSION \n
                                            WHERE 1=1 AND CONSUMEREQNO = \'%1$s\'
                                            ORDER BY CD.CONSUMABLEDEFNAME,CLO.CONSUMABLELOTID
    </string>

    <string name="GetConsumeOutboundQuery"> SELECT CO.REQUESTQTY,CO.OUTQTY,CO.UNIT,CD.CONSUMABLEDEFNAME \n
                                            FROM SF_CONSUMEOUTBOUND CO \n
                                            INNER JOIN SF_CONSUMABLEDEFINITION CD ON CD.CONSUMABLEDEFID = CO.CONSUMABLEDEFID \n
                                                                                 AND CD.CONSUMABLEDEFVERSION = CO.CONSUMABLEDEFVERSION \n
                                            WHERE 1=1 AND CONSUMEREQNO = \'%1$s\'
                                            ORDER BY CD.CONSUMABLEDEFNAME
    </string>

    <!--Shipping-->
    <string name="GetShippingPlanQueryAll"> SELECT SHIPPINGPLANNO,POID,SHIPPINGPLANSEQ,CONTAINERSEQ,     \n
                                                   CUSTOMERID,PRODUCTDEFNAME,PLANQTY,      \n
                                                   CONTAINERSPEC,PLANSTARTTIME,             \n
                                                   PLANENDTIME,STATE                        \n
                                            FROM SF_SHIPPINGPLAN                                \n
                                            WHERE 1=1                                             \n
                                            AND ISOISAVE IS NOT \'Y\'                          \n
                                            AND PLANSTARTTIME &gt; \'%1$s\'                     \n
                                            AND PLANSTARTTIME &lt; \'%2$s\'                     \n
                                            UNION                                                 \n
                                            SELECT SHIPPINGPLANNO,POID,SHIPPINGPLANSEQ,CONTAINERSEQ,      \n
                                                   CUSTOMERID,PRODUCTDEFNAME,PLANQTY,      \n
                                                   CONTAINERSPEC,PLANSTARTTIME,             \n
                                                   PLANENDTIME,STATE                        \n
                                            FROM SF_SHIPPINGPLAN                                \n
                                            WHERE 1=1                                             \n
                                            AND STATE IS NOT \'Finished\'                          \n
                                            AND ISOISAVE = \'Y\'                                 \n
                                            AND PLANSTARTTIME &gt; \'%1$s\'                     \n
                                            AND PLANSTARTTIME &lt; \'%2$s\'                     \n
    </string>

    <string name="GetShippingPlanQueryFinished">
                                            SELECT SP.SHIPPINGPLANNO,POID,SHIPPINGPLANSEQ,CONTAINERSEQ,     \n
                                                   SP.CUSTOMERID,SP.PRODUCTDEFNAME,SP.PLANQTY,      \n
                                                   SP.CONTAINERSPEC,SP.PLANSTARTTIME,             \n
                                                   SP.PLANENDTIME,SP.STATE                        \n
                                            FROM SF_SHIPPINGPLAN SP                               \n
                                            WHERE 1=1                                             \n
                                            AND SP.STATE = \'Finished\'                               \n
                                            AND SP.ISOISAVE IS NOT \'Y\'                            \n
                                            AND SP.PLANSTARTTIME &gt; \'%1$s\'                     \n
                                            AND SP.PLANSTARTTIME &lt; \'%2$s\'                     \n
    </string>
    <string name="GetShippingPlanQueryNormal">
                                            SELECT SP.SHIPPINGPLANNO,POID,SHIPPINGPLANSEQ,CONTAINERSEQ,    \n
                                                   SP.CUSTOMERID,SP.PRODUCTDEFNAME,SP.PLANQTY,      \n
                                                   SP.CONTAINERSPEC,SP.PLANSTARTTIME,             \n
                                                   SP.PLANENDTIME,SP.STATE                        \n
                                            FROM SF_SHIPPINGPLAN SP                               \n
                                            WHERE 1=1                                             \n
                                            AND SP.PLANSTARTTIME &gt; \'%1$s\'                     \n
                                            AND SP.PLANSTARTTIME &lt; \'%2$s\'                     \n
                                            AND SP.STATE = \'%3$s\'                           \n
    </string>
    <string name="gspOrderBy">ORDER BY SHIPPINGPLANNO</string>

    <string name="GetlOTShippingQuery">SELECT  A.LOTID,A.RFID ,A.QTY            \n
                                           FROM SF_LOT A                                          \n
                                        WHERE 1=1                                                 \n
                                        AND A.PURCHASEORDERID = \'%1$s\'                          \n
                                        AND A.LOTSTATE = \'Finished\'                             \n
                                        ORDER BY A.LOTID                                          \n
    </string>

    <string name="GetlOTShipedQuery">SELECT LS.LOTID,LS.CONTAINERNO,LS.SEALNO,LS.QTY    \n
                                           FROM SF_LOTSHIPPING LS                                 \n
                                        WHERE 1=1                                                 \n
                                        AND LS.SHIPPINGPLANNO  = \'%1$s\'                          \n
                                        AND LS.SHIPPINGPLANSEQ  = \'%2$s\'                          \n
                                        AND LS.CONTAINERSEQ  = \'%3$s\'                          \n
                                        AND LS.LOTSTATE = \'Finished\'                             \n
                                        AND LS.VALIDSTATE = \'Valid\'                             \n
                                        ORDER BY A.LOTID                                          \n
    </string>

    <string name="GetShippinglOTQuery">SELECT  A.LOTID,A.RFID ,B.CONTAINERNO ,                    \n
                                               B.SEALNO ,A.QTY,B.VALIDSTATE                       \n
                                           FROM SF_LOT A                                          \n
                                        LEFT JOIN SF_LOTSHIPPING B on B.LOTID = A.LOTID           \n
                                        WHERE 1=1                                                 \n
                                        AND A.SALESORDERID = \'%1$s\'                             \n
                                        AND A.PROCESSSEGMENTID = \'PACKING_01\'                   \n
                                        AND A.LOTSTATE = \'Finished\'                             \n
                                        ORDER BY A.LOTID                                          \n
    </string>

    <string name="GetlOTQtyQuery"> SELECT  A.LOTID ,A.QTY                                         \n
                                       FROM SF_LOT A                                              \n
                                    WHERE 1=1                                                     \n
                                    AND A.SALESORDERID = \'%1$s\'                                 \n
                                    AND A.PROCESSSEGMENTID = \'PACKING_01\'                       \n
                                    AND A.LOTSTATE = \'Finished\'                                 \n
                                    AND A.RFID = \'%2$s\'                                         \n
                                    ORDER BY A.LOTID                                              \n
    </string>

    <string name="GetUploadShipplanQuery"> SELECT  SHIPPINGPLANNO ,STATE             \n
                                                 FROM SF_SHIPPINGPLAN A                           \n
                                           WHERE 1=1                                              \n
                                           AND ISPDASHIPPING = \'Y\'
    </string>

    <string name="GetUploadlOTShipQuery"> SELECT LS.*,L.QTY FROM                                  \n
                                          SF_LOTSHIPPING LS                                       \n
                                          INNER JOIN SF_SHIPPINGPLAN SP                           \n
                                          ON SP.SHIPPINGPLANNO = LS.SHIPPINGPLANNO                \n
                                          INNER JOIN SF_LOT L ON L.LOTID = LS.LOTID               \n
                                          WHERE 1=1                                               \n
                                          AND SP.ISPDASHIPPING = \'Y\'                            \n
    </string>

