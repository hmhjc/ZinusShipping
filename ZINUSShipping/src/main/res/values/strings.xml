                                            AND CD.CONSUMABLEDEFVERSION = CI.CONSUMABLEDEFVERSION \n
                                            WHERE 1=1 AND INBOUNDNO = \'%1$s\'                    \n
                                            ORDER BY CD.CONSUMABLEDEFNAME
    </string>
