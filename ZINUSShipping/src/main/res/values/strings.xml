    <!--Shipping-->
    <string name="GetShippingPlanQueryAll"> SELECT SHIPPINGPLANNO,BOOKINGNO,CUSTOMERID,PLANDATE,  \n
                                                   SHIPPINGPLANDATE,SHIPPINGENDPLANDATE,          \n
                                                   SHIPPINGENDDATE,STATE                         \n
                                            FROM SF_SHIPPINGPLAN                                  \n
                                            WHERE 1=1                                             \n
                                            AND SHIPPINGENDPLANDATE &gt; \'%1$s\'                          \n
                                            AND SHIPPINGENDPLANDATE &lt; \'%2$s\'                          \n
    </string>

    <string name="GetShippingPlanQueryNormal">SELECT SHIPPINGPLANNO,BOOKINGNO,CUSTOMERID,PLANDATE,  \n
                                                   SHIPPINGPLANDATE,SHIPPINGENDPLANDATE,          \n
                                                   SHIPPINGENDDATE,STATE                        \n
                                            FROM SF_SHIPPINGPLAN                                  \n
                                            WHERE 1=1                                             \n
                                            AND STATE = \'%1$s\'                               \n
                                            AND SHIPPINGENDPLANDATE &gt; \'%2$s\'                          \n
                                            AND SHIPPINGENDPLANDATE &lt; \'%3$s\'                          \n
    </string>
    <string name="gspOrderBy">ORDER BY SHIPPINGPLANNO</string>

    <string name="GetPlanSeqQuery">SELECT A.*,(A.PLANQTY - A.SHIPPINGEDQTY) AS NOSHIPPINGEDQTY FROM (  \n
                                SELECT                                    \n
                                     SPD.SHIPPINGPLANSEQ                                   \n
			           	            ,SPD.POID                                   \n
			           	            ,SPD.PLANQTY                                   \n
                                    ,SPD.CONTAINERNO                            \n
                                    ,SPD.SEALNO                                 \n
                                    ,SPD.PRODUCTDEFID                            \n
                                    ,SPD.PRODUCTDEFVERSION                            \n
                                    ,SPD.PRODUCTDEFNAME                      \n
                                    ,SPD.ORDERNO                                  \n
                                    ,SPD.ORDERTYPE                                  \n
                                    ,SPD.LINENO                                  \n
                                    ,CASE WHEN SUM(SL.QTY) IS NULL THEN \'0\' ELSE SUM(SL.QTY) END AS SHIPPINGEDQTY   \n
			           	       FROM SF_SHIPPINGPLANDETAIL SPD                                  \n
                               LEFT JOIN SF_SHIPPINGLOT SL ON SL.SHIPPINGPLANNO = SPD.SHIPPINGPLANNO      \n
														  AND SL.CONTAINERSEQ = SPD.CONTAINERSEQ      \n
													      AND SL.SHIPPINGPLANSEQ = SPD. SHIPPINGPLANSEQ      \n
                                                          AND SL.VALIDSTATE = \'Valid\'                       \n
			           	       WHERE 1 = 1                                   \n
			           	       AND SPD.SHIPPINGPLANNO = \'%1$s\'                                   \n
			           	       AND SPD.CONTAINERSEQ = \'%2$s\'                                   \n
                               GROUP BY SPD.SHIPPINGPLANSEQ                                     \n
			           	       ORDER BY CAST(SPD.SHIPPINGPLANSEQ AS INTEGER), SPD.POID,SPD.PRODUCTDEFID ) A    \n
    </string>

    <string name="GetlOTNoShippingQuery">SELECT       \n
                                                L.LOTID      \n
                                              , L.PURCHASEORDERID      \n
                                              , L.QTY      \n
                                              , L.TRACKOUTTIME      \n
                                              , L.RFID      \n
                                              , L.PRODUCTDEFID      \n
                                              , L.PRODUCTDEFNAME      \n
                                              , L.SHIPPINGPLANSEQ      \n
                                              FROM SF_LOT L      \n
                                              WHERE L.SHIPPINGPLANNO = \'%1$s\'       \n
                                              AND L.CONTAINERSEQ = \'%2$s\'      \n
                                              AND L.SHIPPINGPLANSEQ = \'%3$s\'      \n
                                              AND L.LOTID NOT IN (SELECT LOTID FROM SF_SHIPPINGLOT         \n
                                                                     WHERE VALIDSTATE = \'Valid\' )        \n
                                              ORDER BY L.LOTID                          \n
    </string>

    <string name="GetlOTShippingQuery">    SELECT       \n
                                                L.LOTID      \n
                                              , L.PURCHASEORDERID      \n
                                              , L.QTY      \n
                                              , L.TRACKOUTTIME      \n
                                              , L.RFID      \n
                                              , L.PRODUCTDEFID      \n
                                              , L.PRODUCTDEFNAME      \n
                                              , CASE WHEN L.LOTID IN (     \n
                                                    SELECT LOTID      \n
                                                            FROM SF_SHIPPINGLOT        \n
                                                    WHERE VALIDSTATE = \'Valid\' )      \n
                                              THEN \'Valid\' ELSE \'Invalid\' END AS VALIDSTATE     \n
                                              FROM SF_LOT L      \n
                                              WHERE 1=1       \n
                                              AND L.QTY > 0      \n
                                              AND (L.PURCHASEORDERID || L.PRODUCTDEFID) IN         \n
														(SELECT         \n
														 SPD.POID || SPD.PRODUCTDEFID        \n
														FROM SF_SHIPPINGPLANDETAIL SPD        \n
														WHERE SPD.SHIPPINGPLANNO IN ( \'%1$s\')        \n
                                                        AND SPD.CONTAINERSEQ IN ( \'%2$s\')        \n
														)        \n
    </string>

    <string name="GetShippinglOTQuery">SELECT  A.LOTID,A.RFID ,B.CONTAINERNO ,                    \n
                                               B.SEALNO ,A.QTY,B.VALIDSTATE                       \n
                                           FROM SF_LOT A                                          \n
                                        LEFT JOIN SF_LOTSHIPPING B on B.LOTID = A.LOTID           \n
                                        WHERE 1=1                                                 \n
                                        AND A.SALESORDERID = \'%1$s\'                             \n
                                        AND A.PROCESSSEGMENTID = \'PACKING_01\'                   \n
                                        AND A.LOTSTATE = \'Finished\'                             \n
                                        ORDER BY A.LOTID                                          \n
    </string>

    <string name="GetlOTQtyQuery"> SELECT  A.LOTID ,A.QTY                                         \n
                                       FROM SF_LOT A                                              \n
                                    WHERE 1=1                                                     \n
                                    AND A.SALESORDERID = \'%1$s\'                                 \n
                                    AND A.PROCESSSEGMENTID = \'PACKING_01\'                       \n
                                    AND A.LOTSTATE = \'Finished\'                                 \n
                                    AND A.RFID = \'%2$s\'                                         \n
                                    ORDER BY A.LOTID                                              \n
    </string>

    <string name="GetUploadShipplanQuery"> SELECT   SHIPPINGPLANNO ,CUSTOMERID,BOOKINGNO   \n
                                                   ,PLANDATE,SHIPPINGPLANDATE,SHIPPINGENDPLANDATE  \n
                                                   ,SHIPPINGENDDATE,STATE                          \n
                                                 FROM SF_SHIPPINGPLAN                              \n
                                           WHERE 1=1                                               \n
                                            AND SHIPPINGPLANNO IN                          \n
                                            (                          \n
                                            SELECT DISTINCT SHIPPINGPLANNO                          \n
                                            FROM SF_SHIPPINGPLANDETAIL                          \n
                                            WHERE ISPDASHIPPING = \'Y\'                          \n
                                            )                               \n
    </string>

    <string name="GetUploadShipplanDetailQuery"> SELECT SPD.SHIPPINGPLANNO,SPD.SHIPPINGPLANSEQ,SPD.CONTAINERSEQ    \n
                                                  ,SPD.POID,SPD.CONTAINERSPEC,SPD.CONTAINERNO,SPD.SEALNO    \n
                                                  ,SPD.COMPLETETIME,SPD.PLANQTY,SPD.PRODUCTDEFNAME    \n
                                                  ,SPD.ORDERTYPE,SPD.ORDERNO,SPD.LINENO,SPD.PRODUCTDEFID,SPD.PRODUCTDEFVERSION    \n
                                                  ,IFNULL(SPL.QTY,\'0\') AS LOADEDQTY   \n
                                                  FROM SF_SHIPPINGPLANDETAIL SPD   \n
                                                  LEFT JOIN (                                            \n
                                                                               SELECT SHIPPINGPLANNO   \n
                                                                                    , SHIPPINGPLANSEQ   \n
                                                                                    , CONTAINERSEQ     \n
                                                                                    , SUM(QTY) AS QTY     \n
                                                                               FROM SF_SHIPPINGLOT     \n
                                                                               WHERE 1 = 1     \n
                                                                               AND VALIDSTATE = \'Valid\'     \n
                                                                               GROUP BY SHIPPINGPLANNO, SHIPPINGPLANSEQ, CONTAINERSEQ        \n
                                                                                    ) SPL     \n
                                                                                   ON SPL.SHIPPINGPLANNO = SPD.SHIPPINGPLANNO     \n
                                                                                  AND SPL.SHIPPINGPLANSEQ = SPD.SHIPPINGPLANSEQ     \n
                                                                                  AND SPL.CONTAINERSEQ = SPD.CONTAINERSEQ     \n
                                                                                   WHERE 1=1                                \n
                                                                                    AND SPD.ISPDASHIPPING = \'Y\'           \n
                                                                                ORDER BY SPD.CONTAINERSEQ            \n
    </string>

    <string name="GetUploadlOTShipQuery"> SELECT DISTINCT SL.*,SPD.PRODUCTDEFNAME,L.TRACKOUTTIME FROM                              \n
                                          SF_SHIPPINGLOT SL                           \n
                                          LEFT JOIN SF_LOT L ON L.LOTID = SL.LOTID                 \n
                                          INNER JOIN SF_SHIPPINGPLANDETAIL SPD                 \n
                                          ON SPD.SHIPPINGPLANNO = SL.SHIPPINGPLANNO                  \n
                                          AND SPD.SHIPPINGPLANSEQ = SL.SHIPPINGPLANSEQ                  \n
                                          AND SPD.CONTAINERSEQ = SL.CONTAINERSEQ                  \n
                                          AND SPD.POID = SL.POID                  \n
                                          AND SPD.ISPDASHIPPING = \'Y\'  \n
    </string>

    <string name="checkShippingIsAllUpload"> SELECT COUNT(*)  AS COUNT                         \n
                                                 FROM SF_SHIPPINGPLANDETAIL                              \n
                                           WHERE 1=1                                               \n
                                           AND ISPDASHIPPING = \'Y\'                               \n
    </string>

    <string name="CheckUser"> SELECT USERNAME,PASSWORD,SEED                           \n
                                                 FROM SF_USER                              \n
                                           WHERE 1=1                                               \n
                                           AND USERID = \'%1$s\'                         \n
    </string>

    <string name="GetShippingByTagIDQuery">
                                                SELECT                        \n
                                                SPD.SHIPPINGPLANNO,                        \n
                                                SPD.SHIPPINGPLANSEQ,                        \n
                                                SPD.ORDERTYPE,                        \n
                                                SPD.ORDERNO,                        \n
                                                SPD.LINENO,                        \n
                                                L.PRODUCTDEFID,                        \n
                                                L.PRODUCTDEFNAME,
                                                L.PRODUCTDEFVERSION,                        \n
                                                SPD.CONTAINERSEQ,                        \n
                                                L.PURCHASEORDERID,                        \n
                                                L.LOTID,                        \n
                                                L.RFID,                        \n
                                                L.TRACKOUTTIME,                        \n
                                                L.QTY,                        \n
                                                datetime(CURRENT_TIMESTAMP,\'localtime\') AS SHIPPINGDATE,                        \n
                                                \'Valid\' AS VALIDSTATE                        \n
                                            FROM                        \n
                                                 SF_SHIPPINGPLANDETAIL SPD                        \n
                                                INNER JOIN SF_LOT L                        \n
                                                WHERE  1=1      \n
                                                AND L.RFID = \'%1$s\'      \n
                                                AND (SPD.POID || SPD.PRODUCTDEFID) = (      \n
                                                SELECT PURCHASEORDERID ||PRODUCTDEFID  FROM SF_LOT WHERE RFID = \'%1$s\'      \n
                                                )      \n
                                                AND SPD.CONTAINERSEQ =  \'%2$s\'     \n
                                                AND L.LOTID NOT IN (      \n
                                                SELECT LOTID FROM SF_SHIPPINGLOT      \n
                                                WHERE VALIDSTATE = \'Valid\'      \n
                                                )               \n
    </string>


    <string name="InsertIntoShippingLotQuery">
                                     INSERT INTO SF_SHIPPINGLOT                           \n
                                        (                           \n
                                        SHIPPINGPLANNO,                           \n
                                        SHIPPINGPLANSEQ,                           \n
                                        ORDERTYPE,                           \n
                                        ORDERNO,                           \n
                                        LINENO,                           \n
                                        PRODUCTDEFID,                           \n
                                        PRODUCTDEFVERSION,                           \n
                                        CONTAINERSEQ,                           \n
                                        POID,                           \n
                                        LOTID,                           \n
                                        QTY,                           \n
                                        SHIPPINGDATE,                           \n
                                        VALIDSTATE                            \n
                                        )                            \n
                                        SELECT                             \n
                                        SPD.SHIPPINGPLANNO,                            \n
                                        SPD.SHIPPINGPLANSEQ,                            \n
                                        SPD.ORDERTYPE,                            \n
                                        SPD.ORDERNO,                            \n
                                        SPD.LINENO,                            \n
                                        L.PRODUCTDEFID,                            \n
                                        L.PRODUCTDEFVERSION,                            \n
                                        SPD.CONTAINERSEQ,                            \n
                                        L.PURCHASEORDERID,                            \n
                                        L.LOTID,                            \n
                                        L.QTY,                            \n
                                        datetime(CURRENT_TIMESTAMP,\'localtime\') AS SHIPPINGDATE,       \n
                                        \'Valid\' AS VALIDSTATE       \n
                                         FROM        \n
                                         SF_SHIPPINGPLANDETAIL SPD       \n
                                        INNER JOIN SF_LOT L       \n
                                        WHERE  1=1       \n
                                        AND L.RFID = \'%1$s\'       \n
                                        and (SPD.POID || SPD.PRODUCTDEFID) = (       \n
                                        SELECT PURCHASEORDERID ||PRODUCTDEFID  FROM SF_LOT WHERE RFID = \'%1$s\'         \n
                                        )         \n
                                        AND L.LOTID NOT IN (         \n
                                        SELECT LOTID FROM SF_SHIPPINGLOT         \n
                                        WHERE VALIDSTATE = \'Valid\'         \n
                                        )         \n
                                        limit 0,1         \n
    </string>

    <string name="UpdateShippingPlanDetailQuery">
                                     UPDATE SF_SHIPPINGPLANDETAIL     \n
                                                 SET ISPDASHIPPING = \'Y\'     \n
                                             WHERE 1=1     \n
                                             AND SHIPPINGPLANNO = \'%1$s\'     \n
                                             AND CONTAINERSEQ = \'%2$s\'     \n
                                             AND SHIPPINGPLANSEQ IN (     \n
                                             SELECT SHIPPINGPLANSEQ  FROM SF_SHIPPINGLOT WHERE LOTID = \'%3$s\'     \n
                                                )     \n
    </string>

