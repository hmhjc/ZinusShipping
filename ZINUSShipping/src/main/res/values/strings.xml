    <!--SpinnerQuery-->
    <string name="codeSpinner">SELECT * FROM SF_CODE WHERE CODECLASSID = \'%1$s\' and LANGUAGETYPE = \'%2$s\'</string>
    <string name="WarehouseSpinner">SELECT * FROM SF_WAREHOUSE \n ORDER BY WAREHOUSEID</string>
    <string name="AreaSpinner">SELECT AREAID,AREANAME FROM SF_AREA \n ORDER BY AREAID</string>
    <string name="ConsumabledefIDSpinner">SELECT distinct CONSUMABLEDEFID FROM SF_CONSUMABLEDEFINITION ORDER BY CONSUMABLEDEFID</string>
    <string name="ShippingPlanSEQSpinner">SELECT distinct SHIPPINGPLANSEQ                          \n
                                          FROM SF_SHIPPINGPLANDETAIL                               \n
                                          WHERE 1 = 1                                       \n
				                          AND SHIPPINGPLANNO = \'%1$s\'                 \n
				                          ORDER BY SHIPPINGPLANSEQ
    </string>

    <string name="ContainerSEQSpinner">SELECT                                      \n
                                        CONTAINERSEQ                                            \n
				                      , CONTAINERSEQ || \' (QTY : \' || SUM(PLANQTY) || \')\' AS CODENAME  \n
                                      , MAX(CONTAINERNO) AS CONTAINERNO
                                      , MAX(SEALNO) AS SEALNO
                                      FROM SF_SHIPPINGPLANDETAIL                                   \n
				                      WHERE 1 = 1                                   \n
				                        AND SHIPPINGPLANNO = \'%1$s\'                                   \n
				                        GROUP BY SHIPPINGPLANNO , CONTAINERSEQ                                   \n
                                        ORDER BY CAST(CONTAINERSEQ AS INTEGER)
    </string>

    <!--StockCheck-->
    <string name="GetStockCheckQuery">SELECT * FROM SF_STOCKCHECK \n WHERE 1=1 \n AND CHECKMONTH  = \'%1$s\' \n</string>
    <string name="gscConditionWarehouse">AND WAREHOUSEID = \'%1$s\' \n</string>
    <string name="gscConditionState">AND STATE = \'%1$s\' \n</string>
    <string name="gscOrderBy">ORDER BY CHECKMONTH,WAREHOUSEID,STATE</string>

    <string name="GetStockCheckDetailQuery">SELECT SC.WAREHOUSEID, SC.UNIT, SC.QTY, SC.CHECKQTY, CD.CONSUMABLEDEFNAME \n
                                            FROM SF_STOCKCHECKDETAIL SC \n
                                            INNER JOIN SF_CONSUMABLEDEFINITION CD ON CD.CONSUMABLEDEFID = SC.CONSUMABLEDEFID \n
                                                                                 AND CD.CONSUMABLEDEFVERSION = SC.CONSUMABLEDEFVERSION \n
                                            WHERE 1=1 AND CHECKMONTH = \'%1$s\' AND WAREHOUSEID = \'%2$s\'
    </string>

    <string name="GetStockLotCheckDetailQuery">SELECT SLC.WAREHOUSEID, SLC.CONSUMABLELOTID, SLC.UNIT, SLC.QTY, SLC.CHECKQTY, CD.CONSUMABLEDEFNAME \n
                                               FROM SF_STOCKLOTCHECKDETAIL SLC \n
                                               INNER JOIN SF_CONSUMABLEDEFINITION CD ON CD.CONSUMABLEDEFID = SLC.CONSUMABLEDEFID \n
                                                                                    AND CD.CONSUMABLEDEFVERSION = SLC.CONSUMABLEDEFVERSION \n
                                            WHERE 1=1 AND CHECKMONTH = \'%1$s\' AND WAREHOUSEID = \'%2$s\'
    </string>

