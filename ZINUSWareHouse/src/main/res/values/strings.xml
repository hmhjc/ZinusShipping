    <string name="codeSpinner">SELECT * FROM SF_CODE WHERE CODECLASSID = \'%1$s\' and LANGUAGETYPE = \'%2$s\'</string>
    <string name="WarehouseSpinner">SELECT WAREHOUSEID ,WAREHOUSENAME FROM SF_WAREHOUSE \n ORDER BY WAREHOUSEID</string>
    <string name="AreaSpinner">SELECT AREAID,AREANAME FROM SF_AREA \n ORDER BY AREAID</string>
