    <!--StockCheck-->
    <string name="GetStockCheckQuery">SELECT * FROM SF_STOCKCHECK \n WHERE 1=1 \n AND CHECKMONTH  = \'%1$s\' \n</string>
    <string name="gscConditionWarehouse">AND WAREHOUSEID = \'%1$s\' \n</string>
    <string name="gscConditionState">AND STATENAME = \'%1$s\' \n</string>
    <string name="gscOrderBy">ORDER BY CHECKMONTH,WAREHOUSEID,STATENAME</string>

    <string name="GetStockCheckDetailQuery">SELECT SC.WAREHOUSEID, SC.UNIT, SC.QTY, SC.CHECKQTY, SC.CONSUMABLEDEFNAME \n
                                                   ,SC.CONSUMABLEDEFID,SC.CONSUMABLEDEFVERSION,SC.CHECKMONTH     \n
                                            FROM SF_STOCKCHECKDETAIL SC \n
                                            WHERE 1=1 AND CHECKMONTH = \'%1$s\' AND WAREHOUSEID = \'%2$s\'
    </string>

    <string name="GetStockLotCheckDetailQuery">SELECT SL.*,U.USERNAME FROM SF_STOCKLOTCHECKDETAIL SL
                                              LEFT JOIN SF_USER U ON U.USERID = SL.USERID  \n
                                            WHERE 1=1 AND SL.CONSUMABLEDEFID = \'%1$s\' AND SL.CONSUMABLEDEFVERSION = \'%2$s\' \n
                                            ORDER BY SL.CONSUMABLELOTID
    </string>

    <string name="GetStockCheckDetailQTYQuery">SELECT SUM(CHECKQTY)  AS SUM \n
                                               FROM SF_STOCKLOTCHECKDETAIL  \n
                                            WHERE 1=1 AND CONSUMABLEDEFID = \'%1$s\' AND CONSUMABLEDEFVERSION = \'%2$s\' \n
    </string>

    <string name="GetConsumeInboundQTYQuery">SELECT SUM(INQTY)  AS SUM \n
                                               FROM SF_CONSUMELOTINBOUND  \n
                                            WHERE 1=1 AND CONSUMABLEDEFID = \'%1$s\' AND CONSUMABLEDEFVERSION = \'%2$s\' \n
    </string>

    <!--Inbound-->
    <string name="GetInboundOrderQuery">SELECT * FROM SF_INBOUNDORDER \n
                                                 WHERE 1=1 \n
                                                 AND SCHEDULEDATE  &gt; \'%1$s\'               \n
                                                 AND SCHEDULEDATE &lt;  \'%2$s\'              \n
                                              </string>
    <string name="gioConditionWAREHOUSEID">AND WAREHOUSEID = \'%1$s\' \n</string>
    <string name="gioConditionState">AND INBOUNDSTATE = \'%1$s\' \n</string>
    <string name="gioOrderBy">ORDER BY INBOUNDNO,WAREHOUSEID,INBOUNDSTATE</string>

    <string name="GetConsumeLotInboundQuery">SELECT *   \n
                                            FROM SF_CONSUMELOTINBOUND  \n
                                            WHERE 1=1 AND CONSUMABLEDEFID = \'%1$s\'             \n
                                            AND CONSUMABLEDEFVERSION = \'%2$s\'             \n
                                            ORDER BY CONSUMABLELOTID             \n
    </string>

    <string name="GetConsumeInboundQuery">SELECT *                                           \n
                                            FROM SF_CONSUMEINBOUND                              \n
                                            WHERE 1=1 AND INBOUNDNO = \'%1$s\'                    \n
                                            ORDER BY CONSUMABLEDEFID
    </string>
    <string name="GetUploadInboundOrderQuery">SELECT * FROM SF_INBOUNDORDER                       \n
                                              WHERE 1=1  AND ISPDASAVE = \'Y\'
    </string>

    <string name="GetUploadConsumeInboundQuery">SELECT  CI.*                                      \n
                                                FROM SF_CONSUMEINBOUND CI                         \n
                                                INNER JOIN SF_INBOUNDORDER IO ON IO.INBOUNDNO = CI.INBOUNDNO    \n
                                                 AND IO.ISPDASAVE = \'Y\'
    </string>

    <string name="GetUploadConsumeLotInboundQuery">SELECT  CLI.*                                      \n
                                                FROM SF_CONSUMELOTINBOUND CLI                         \n
                                                INNER JOIN SF_INBOUNDORDER IO ON IO.INBOUNDNO = CLI.INBOUNDNO    \n
                                                 AND IO.ISPDASAVE = \'Y\'
    </string>

    <string name="GetUploadStockCheckQuery">SELECT                              \n
                                                  WAREHOUSEID                              \n
                                                , WAREHOUSENAME                             \n
                                                , CHECKMONTH                             \n
                                                , STATE                             \n
                                                , STATENAME                             \n
                                                , STARTDATE                             \n
                                                , ENDDATE                             \n
                                             FROM SF_STOCKCHECK                             \n
                                             WHERE ISPDASAVE = \'Y\'                             \n
    </string>

    <string name="GetUploadStockCheckDetailQuery">SELECT     \n
                                                      SCD.CONSUMABLEDEFID     \n
                                                    , SCD.CONSUMABLEDEFNAME     \n
                                                    , SCD.CONSUMABLEDEFVERSION     \n
                                                    , SCD.UNIT     \n
                                                    , SCD.QTY     \n
                                                    , SCD.CHECKQTY     \n
                                                    , SCD.WAREHOUSEID     \n
                                                    , SCD.CHECKMONTH     \n
                                                    , SCD.USERID     \n
                                                    , U.USERNAME     \n
                                                    FROM     \n
                                                    SF_STOCKCHECKDETAIL SCD     \n
                                                    LEFT JOIN SF_USER U ON U.USERID = SCD.USERID     \n
                                                    WHERE SCD.CHECKMONTH IN (SELECT CHECKMONTH FROM     \n
                                                                             SF_STOCKCHECK     \n
                                                                              WHERE ISPDASAVE = \'Y\')     \n
                                                    AND SCD.WAREHOUSEID IN (SELECT WAREHOUSEID FROM     \n
                                                                             SF_STOCKCHECK     \n
                                                                              WHERE ISPDASAVE = \'Y\')     \n
    </string>

    <string name="GetUploadStockLotCheckDetailQuery">SELECT     \n
                                                      SLD.CONSUMABLELOTID     \n
                                                    , SLD.CONSUMABLEDEFID     \n
                                                    , SLD.CONSUMABLEDEFNAME     \n
                                                    , SLD.CONSUMABLEDEFVERSION     \n
                                                    , SLD.UNIT     \n
                                                    , SLD.QTY     \n
                                                    , SLD.CHECKQTY     \n
                                                    , SLD.WAREHOUSEID     \n
                                                    , SLD.CHECKMONTH     \n
                                                    , SLD.USERID     \n
                                                    , U.USERNAME     \n
                                                    FROM     \n
                                                    SF_STOCKLOTCHECKDETAIL SLD     \n
                                                    LEFT JOIN SF_USER U ON U.USERID = SLD.USERID     \n
                                                    WHERE SLD.CHECKMONTH IN (SELECT CHECKMONTH FROM     \n
                                                                             SF_STOCKCHECK     \n
                                                                              WHERE ISPDASAVE = \'Y\')     \n
                                                    AND SLD.WAREHOUSEID IN (SELECT WAREHOUSEID FROM     \n
                                                                             SF_STOCKCHECK     \n
                                                                              WHERE ISPDASAVE = \'Y\')     \n
    </string>

    <!--ConsumeRequest-->
    <string name="GetConsumeRequestQuery">SELECT * FROM SF_CONSUMEREQUEST \n WHERE 1=1 \n AND REQUESTDATE  &gt; \'%1$s\' AND REQUESTDATE &lt;  \'%2$s\' \n</string>
    <string name="gcrConditionLocationId">AND LOCATIONID = \'%1$s\' \n</string>
    <string name="gcrConditionState">AND OUTBOUNDSTATE = \'%1$s\' \n</string>
    <string name="gcrOrderBy">ORDER BY CONSUMEREQNO,LOCATIONID</string>

    <string name="GetConsumeLotOutboundQuery">SELECT CLO.REQUESTQTY,CLO.OUTQTY,CLO.UNIT,CLO.CONSUMABLELOTID,CD.CONSUMABLEDEFNAME \n
                                            FROM SF_CONSUMELOTOUTBOUND CLO \n
                                            INNER JOIN SF_CONSUMABLEDEFINITION CD ON CD.CONSUMABLEDEFID = CLO.CONSUMABLEDEFID \n
                                                                                 AND CD.CONSUMABLEDEFVERSION = CLO.CONSUMABLEDEFVERSION \n
                                            WHERE 1=1 AND CONSUMEREQNO = \'%1$s\'
                                            ORDER BY CD.CONSUMABLEDEFNAME,CLO.CONSUMABLELOTID
    </string>

    <string name="GetConsumeOutboundQuery"> SELECT CO.REQUESTQTY,CO.OUTQTY,CO.UNIT,CD.CONSUMABLEDEFNAME \n
                                            FROM SF_CONSUMEOUTBOUND CO \n
                                            INNER JOIN SF_CONSUMABLEDEFINITION CD ON CD.CONSUMABLEDEFID = CO.CONSUMABLEDEFID \n
                                                                                 AND CD.CONSUMABLEDEFVERSION = CO.CONSUMABLEDEFVERSION \n
                                            WHERE 1=1 AND CONSUMEREQNO = \'%1$s\'
                                            ORDER BY CD.CONSUMABLEDEFNAME
    </string>

    <!--Shipping-->
    <string name="GetShippingPlanQuery"> SELECT SP.SHIPPINGPLANNO,SP.PRODUCTIONORDERNAME,         \n
                                                SP.CUSTOMERID,SP.PRODUCTDEFID,SP.PLANQTY,         \n
                                                SP.CONTAINERSPEC,SP.PLANSTARTTIME,                \n
                                                SP.PLANENDTIME,SP.STATENAME                           \n
                                         FROM SF_SHIPPINGPLAN SP                                  \n
                                         WHERE 1=1                                                \n
                                         AND SP.PLANSTARTTIME &gt; \'%1$s\'                        \n
                                         AND SP.PLANSTARTTIME &lt; \'%2$s\'                        \n

    </string>
    <string name="gspState">AND SP.STATENAME = \'%1$s\' \n</string>
    <string name="gspOrderBy">ORDER BY SP.SHIPPINGPLANNO</string>

    <string name="GetlOTShippingQuery"> SELECT  LOTID                                             \n
                                         FROM SF_LOT                                              \n
                                         WHERE 1=1                                                \n
                                         AND SALESORDERID = \'%1$s\'                              \n
                                         AND PROCESSSEGMENTID = \'PACKING_01\'                      \n
                                         AND LOTSTATE = \'Finished\'                                \n
                                         ORDER BY LOTID
    </string>

    <string name="GetlOTQtyQuery"> SELECT  L.LOTID,L.QTY                                          \n
                                         FROM SF_LOT L                                            \n
                                         INNER JOIN SF_RFIDTAG T ON T.LOTID = L.LOTID             \n
                                         WHERE 1=1                                                \n
                                         AND T.TAGID = \'%1$s\'                                   \n
    </string>

    <string name="checkStockCheckIsAllUpload"> SELECT COUNT(*)    AS COUNT                            \n
                                                 FROM SF_STOCKCHECK                              \n
                                           WHERE 1=1                                               \n
                                           AND ISPDASAVE = \'Y\'                               \n
    </string>

    <string name="checkInboundIsAllUpload"> SELECT COUNT(*)    AS COUNT                            \n
                                                 FROM SF_INBOUNDORDER                              \n
                                           WHERE 1=1                                               \n
                                           AND ISPDASAVE = \'Y\'                               \n
    </string>

    <string name="checkIsInStockLotCheck"> SELECT COUNT(*)    AS COUNT                            \n
                                                 FROM SF_STOCKLOTCHECKDETAIL                              \n
                                           WHERE 1=1                                               \n
                                           AND CONSUMABLEDEFID = \'%1$s\'  AND CONSUMABLEDEFVERSION =  \'%2$s\' \n
    </string>
    <string name="checkisInconsumeLotInbound"> SELECT COUNT(*)    AS COUNT                            \n
                                                 FROM SF_CONSUMELOTINBOUND                              \n
                                           WHERE 1=1                                               \n
                                           AND CONSUMABLEDEFID = \'%1$s\'  AND CONSUMABLEDEFVERSION =  \'%2$s\' \n
    </string>

    <string name="CheckUser"> SELECT USERNAME,PASSWORD,SEED                           \n
                                                 FROM SF_USER                              \n
                                           WHERE 1=1                                               \n
                                           AND USERID = \'%1$s\'                         \n
    </string>

